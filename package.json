{
	"name": "sandfox-gen-jpa",
	"version": "1.0.7",
	"description": "Spring Boot (JPA, Kotlin, Data Rest) project generator. Based on json (Google Spreadsheet) config. Using EJS templates.",
	"main": "dist/index.js",
	"bin": {
		"sandfox-gen-jpa": "./dist/main.js"
	},
	"scripts": {
		"start": "node dist/index.js",
		"prestart": "npm run build",
		"build": "tsc -p src",
		"watch": "tsc -p src -w",
		"format": "prettier --config ./.prettierrc --write \"src/**/*.ts\" \"tests/**/*.ts\"",
		"lint": "tslint -p src/tsconfig.json -c tslint.json",
		"test": "jest",
		"coverage": "jest --coverage"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run format"
		}
	},
	"keywords": [
		"typescript",
		"generator",
		"kotlin",
		"jpa",
		"spring"
	],
	"author": "eGodzzo",
	"license": "ISC",
	"types": "dist/index.d.ts",
	"dependencies": {
		"chalk": "^2.4.2",
		"command-line-args": "^5.1.1",
		"ejs": "^3.1.5",
		"gdut-gsheet": "^0.1.3",
		"md5": "^2.3.0",
		"pluralize": "^8.0.0"
	},
	"devDependencies": {
		"@types/command-line-args": "^5.0.0",
		"@types/ejs": "^3.0.4",
		"@types/jest": "^26.0.18",
		"@types/md5": "^2.2.0",
		"@types/node": "^12.20.19",
		"@types/pluralize": "0.0.29",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"prettier": "^2.3.2",
		"ts-jest": "^26.4.4",
		"tslint": "^6.1.3",
		"typescript": "^4.3.5"
	}
}
